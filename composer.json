{
    "name": "lyne007/auth-recaptcha",
    "description": "Add Google reCAPTCHA for laravel-admin.",
    "type": "library",
    "keywords": ["laravel-admin", "extension","auth-recaptcha"],
    "homepage": "https://github.com/lyne007/auth-recaptcha",
    "license": "MIT",
    "authors": [
        {
            "name": "lyne007",
            "email": "lyne007@163.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Lyne007\\AuthRecaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lyne007\\AuthRecaptcha\\AuthRecaptchaServiceProvider"
            ]
        }
    }
